<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jhon.calculator">

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Options.ProbActivity" />
        <activity android:name=".History" />
        <activity android:name=".Options.MulActivity" />
        <activity android:name=".Options.AddSubActivity" />
        <activity android:name=".Options.Factorial" />
        <activity android:name=".Options.PowActivity" />
        <activity android:name=".Options.DivActivity" />
        <activity android:name=".Options.InteActivity" />
        <activity
            android:name=".Utils.IntroActivity"
            android:theme="@style/Theme.Design.BottomSheetDialog" />
        <activity android:name=".Options.MixActivity"></activity>
    </application>

</manifest>